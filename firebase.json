      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "toyproject-mypage",
          "configurations": {
            "android": "1:301811027373:android:c2290180eb4588eb22e524",
            "ios": "1:301811027373:ios:87a17a1d4e13532b22e524",
            "web": "1:301811027373:web:34913d353cf5282d22e524"
          }
        }
      }
    }
  }
