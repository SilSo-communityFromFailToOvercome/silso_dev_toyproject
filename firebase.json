      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "toyproject-mypage",
          "appId": "1:301811027373:android:c2290180eb4588eb22e524",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "toyproject-mypage",
          "appId": "1:301811027373:ios:87a17a1d4e13532b22e524",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
